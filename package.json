{
	"name": "@pouchlab/realtor",
	"version": "1.0.1",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pouchlabs/pouchrealtor.git"
	},
	"bugs": {
		"url": "https://github.com/pouchlabs/pouchrealtor/issues"
	},
	"author": {
		"name": "pouchlabs"
	},
	"contributors": [
		{
			"name": "pouchlabs"
		}
	],
	"sponsor": {
		"url": "https://ko-fi.com/pouchlabs"
	},
	"description": "a blazing fast socket.io alternative,pure websockets",
	"scripts": {
		"dev": "vite dev",
		"start": "node server.js",
		"build": "vite build && npm run package && npm start",
		"preview": "node server.js",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
		"test": "npm run test:integration && npm run test:unit",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test:integration": "playwright test",
		"test:unit": "vitest",
		"dev:server": "node  --watch --trace-warnings ./src/lib/bin/pouchrealtor.js && dev"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"devDependencies": {
		"@playwright/test": "^1.28.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-node": "^5.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/package": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.0",
		"eruda": "^3.0.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"polka": "^0.5.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"publint": "^0.1.9",
		"sass": "^1.72.0",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.11",
		"vitest": "^1.2.0"
	},
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"dependencies": {
		"@msgpack/msgpack": "^3.0.0-beta2",
		"detect-port": "^1.5.1",
		"esm-env": "^1.0.0",
		"eventemitter3": "^5.0.1",
		"is-online": "^10.0.0",
		"isomorphic-ws": "^5.0.0",
		"kleur": "^4.1.5",
		"ms": "^2.1.3",
		"nanoid": "^5.0.6",
		"ws": "^8.16.0"
	},
	"keywords": [
		"realtime,ws,sync,pub sub,websocket,socket.io,realtor"
	]
}
